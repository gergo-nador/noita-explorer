{
  "name": "@noita-explorer/tools",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "dist/main.js",
  "module": "dist/main.js",
  "types": "dist/main.d.ts",
  "exports": {
    "./common": {
      "import": "./dist/common.js",
      "types": "./dist/common/main.d.ts"
    },
    "./lua": {
      "import": "./dist/lua.js",
      "types": "./dist/lua/main.d.ts"
    },
    "./xml": {
      "import": "./dist/xml.js",
      "types": "./dist/xml/main.d.ts"
    },
    ".": {
      "import": "./dist/main.js",
      "default": "./dist/main.js"
    }
  },
  "scripts": {
    "dev": "vite build --watch",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "generate-barrels": "barrelsby --config barrels.config.json"
  },
  "devDependencies": {
    "@types/luaparse": "^0.2.12",
    "@types/xml2js": "^0.4.14",
    "barrelsby": "^2.8.1",
    "typescript": "~5.6.2",
    "vite": "^6.0.1",
    "vite-plugin-dts": "^4.3.0"
  },
  "dependencies": {
    "@noita-explorer/model": "*",
    "events": "^3.3.0",
    "luaparse": "^0.3.1",
    "xml2js": "^0.6.2"
  }
}
