{
  "name": "@noita-explorer/web",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "prebuild": "sh scripts/ci.sh",
    "build": "tsc -b && vite build",
    "postbuild": "sh scripts/ci-post.sh",
    "lint": "eslint .",
    "preview": "vite preview",
    "production-server-local": "cd dist && python3 ../scripts/http-server.py",
    "data-wak-download": "npx tsx scripts/data-wak/download.ts -o dist-tmp",
    "scrape-data-wak": "npx tsx scripts/data-wak/scrape.ts -t dist-tmp/common.csv --data-wak dist-tmp/data.wak --o-wak public/noita_wak_data.json --o-gif dist-tmp/noita_data_gifs.json",
    "generate-gifs": "npx tsx scripts/generate-gifs/index.ts --gif-data dist-tmp/noita_data_gifs.json -o public",
    "generate-static-assets": "npx tsx scripts/generate-seo-sites-for-wiki/index.ts --wak-data public/noita_wak_data.json -o public",
    "generate-sitemap": "npx tsx scripts/sitemap/sitemap-text.ts -o public/sitemap.txt",
    "generate-statis-site": "npx tsx scripts/ssg/index.ts"
  },
  "dependencies": {
    "@noita-explorer/file-systems": "*",
    "@noita-explorer/model": "*",
    "@noita-explorer/model-noita": "*",
    "@noita-explorer/noita-component-library": "*",
    "@noita-explorer/react-utils": "*",
    "@noita-explorer/scrapers": "*",
    "@noita-explorer/tools": "*",
    "@sentry/react": "^9.29.0",
    "@tsparticles/plugin-emitters": "^3.8.1",
    "@tsparticles/react": "^3.0.0",
    "@tsparticles/slim": "^3.8.1",
    "buffer": "^6.0.3",
    "diff": "^8.0.2",
    "idb": "^8.0.1",
    "plotly.js": "^2.35.3",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-plotly.js": "^2.6.0",
    "react-router-dom": "^7.0.2",
    "react-use": "^17.6.0",
    "tsparticles": "^3.8.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.15.0",
    "@sentry/cli": "^2.47.1",
    "@types/jsdom": "^21.1.7",
    "@types/minimist": "^1.2.5",
    "@types/react": "19.1.8",
    "@types/react-dom": "19.1.6",
    "@types/react-plotly.js": "^2.6.3",
    "@vitejs/plugin-react": "^4.3.4",
    "dotenv": "^17.0.1",
    "eslint": "^9.15.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "fake-indexeddb": "^6.0.1",
    "globals": "^15.12.0",
    "jsdom": "^26.1.0",
    "minimist": "^1.2.8",
    "sharp": "^0.34.2",
    "tsx": "^4.20.3",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.15.0",
    "vite": "^6.1.0",
    "vite-plugin-mkcert": "^1.17.6"
  }
}
